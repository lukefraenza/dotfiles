alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias mkdir='mkdir -p'
alias du='du -kh'       # Makes a more readable output.
alias df='df -kTh'
alias h='history'
alias j='jobs -l'
alias which='type -a'

alias ls='ls -hF --color'  # add colors for filetype recognition

alias more='less'
